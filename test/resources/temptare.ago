ordo primor {
    des satus() {
        xa := II
        yae := -1.2
        yae := 1.3

        lego()
        werum := lego().tondeo().decido(", ")

        pro ies in werum[0 .< werum[0..5].diu() - II] {
            si ia ^ I > 0 {
                dixit(ies)
            }

            si iae + -.1 > 0 {
                dixit(ies)
            } aluid (iam et falsus) < 0 {
